
    <table class="table table-striped">
        <% events.forEach(function(model){ %>
        <tr>
            <td>
                <%=model.id %>
            </td>
            <td>
                <a href="create/<%=model.id %>">
                    <%=model.name %></a>
            </td>
        </tr>
        <%}); %>
    </table>

